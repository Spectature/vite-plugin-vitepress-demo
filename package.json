{
  "name": "vite-plugin-vitepress-demo",
  "version": "2.0.0",
  "description": "vite plugin vitepress code block demo",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./theme": {
      "import": "./dist/theme.js"
    },
    "./components/*": {
      "import": "./dist/components/*"
    }
  },
  "scripts": {
    "build": "run-s clean ts:build un:build",
    "clean": "rimraf dist",
    "un:build": "unbuild",
    "ts:build": "tsup",
    "docs:dev": "vitepress dev example",
    "docs:build": "vitepress build example",
    "docs:preview": "vitepress serve example"
  },
  "keywords": [
    "vitepress",
    "markdown",
    "vite",
    "vite-plugin"
  ],
  "author": "aibayanyu",
  "license": "MIT",
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.0.2",
    "@mistjs/tsconfig-vue": "^0.0.3",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.2",
    "eslint": "^8.25.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vite": "^3.1.8",
    "vitepress": "1.0.0-alpha.21",
    "vue": "^3.2.41"
  },
  "peerDependencies": {
    "vitepress": "1.0.0-alpha.21",
    "vue": "^3.2.41"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "dependencies": {
    "markdown-it": "^13.0.1"
  }
}
